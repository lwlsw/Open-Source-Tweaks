<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>ExampleCepheiProjectPrefs First Page</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<!-- this defaults key needs to match the identifier you registered with your HBPreferences -->
			<key>defaults</key>
			<string>com.lacertosusrepo.examplecepheiprojectprefs</string>
			<!-- this key needs to match the key you registered with your HBPreferences -->
			<key>key</key>
			<string>exampleSwitch</string>
			<key>label</key>
			<string>Example Switch</string>
			<!-- PostNotification will send a notification to Cephei to update your preferences without a respring, not necessary but nice for the user -->
			<!-- the string for the key will just be your preferences identifier with /ReloadPrefs on the end -->
			<key>PostNotification</key>
			<string>com.lacertosusrepo.examplecepheiprojectprefs/ReloadPrefs</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>action</key>
			<string>respringMethod</string>
			<key>label</key>
			<string>Respring!</string>
		</dict>
	</array>
	<key>title</key>
	<string>ExampleCepheiProjectPrefs</string>
</dict>
</plist>
